{
  "fileTypes": ["eml"],
  "name": "Embedded Ocaml",
  "patterns": [
    {
      "name": "source.ocaml.embedded",
      "match": "(?<=<%)(.+)?(?=%>)",
      "captures": {
        "0": {
          "name": "source.ocaml",
           "patterns": [
        {
          "include": "source.ocaml"
        }
      ]
        }
      }
     
    }
  ],
  "scopeName": "text.ocaml"
}